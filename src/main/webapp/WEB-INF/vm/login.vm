<title>质量检测系统登录</title>
<link href="css/login.css" type="text/css" rel="stylesheet"> 
<script src="js/jquery-1.12.3.min.js" language="javascript"
    type="text/javascript"></script>
<script src="js/login.js" language="javascript"
    type="text/javascript"></script>
</head> 
<body> 
<div class="login">
    <div class="message">质量检测系统登录</div>
    <div id="darkbannerwrap"></div>
    <form method="post" name="form_login" onSubmit="return check(form_login)">
		<input name="username" placeholder="用户名"  id="username" type="text"/>
		<hr class="hr15">
		<input name="password" placeholder="密码"  id="password" type="password"/>
        <div style="height:20px;">
        <span id="tip" style="color:red;"></span>
        </div>
		<input value="登录" style="width:100%;" type="submit">
	</form>	
</div>

</body>
</html>

